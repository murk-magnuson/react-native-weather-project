{
  "expo": {
    "name": "Weather",
    "slug": "Weather",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A pretty basic weather application following a tutorial from handlebarlabs.com",
    "android": {
      "package": "mmagnuson.hbl.weather",
      "versionCode": 1
    }
  }
}